  "description": "A lightweight library to make authoring serverless, Panda Sky powered APIs easier.",
  "main": "./lib/index.js",
  "files": [
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "node_modules/.bin/coffee --compile --transpile --inline-map --output lib/ src/",
